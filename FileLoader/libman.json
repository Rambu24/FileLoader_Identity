{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@6.2.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "sweetalert@2.1.2",
      "destination": "wwwroot/lib/sweetalert/"
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "js/dataTables.bootstrap.min.js",
        "css/dataTables.bootstrap.min.css",
        "css/jquery.dataTables.min.css"
      ]
    },
    {
      "library": "Chart.js@4.1.2",
      "destination": "wwwroot/lib/Chart.js/dist/"
    }
  ]
}